    "@biconomy/common": "^3.1.4",
    "@biconomy/paymaster": "^3.1.4",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@tanstack/react-query": "^5.59.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "react-tracked": "^2.0.1",
    "viem": "^2.21.10",
    "wagmi": "^2.12.8",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
