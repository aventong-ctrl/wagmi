{
  "name": "wagmi-viem-tutorial",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "基于 Vite + React + TypeScript 的 wagmi + viem 项目",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview --port 5173",
    "typecheck": "tsc -b --pretty",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "keywords": [
    "wagmi",
    "viem",
    "web3",
    "vite",
    "react",
    "tutorial"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.12.3",
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query": "^5.59.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "viem": "^2.21.10",
    "wagmi": "^2.12.8"
  },
  "devDependencies": {
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json}": [
      "prettier --write"
    ]
  }
}
